{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "global-require":"off",
        "no-unused-vars":"off",
        "no-underscore-dangle":"off",
        "no-console":"off",
        "no-param-reassign":"off",
        "no-restricted-syntax":"off",
        "guard-for-in":"off",
        "no-restricted-globals":"off",
        "no-alert":"off",
        "import/no-extraneous-dependencies":"off"
    }
}
